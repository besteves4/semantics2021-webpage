@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix dpv: <http://www.w3.org/ns/dpv#> .
@prefix acl:  <http://www.w3.org/ns/auth/acl#> .
@prefix odrl-ac: <https://example.com/odrl-ac> .

:ex-2
    a odrl:Policy ;
    odrl:prohibition [
        a odrl:Prohibition ;
        odrl:assigner <https://beatriz.databox.me/profile/card#me> ;
        odrl:target dpv:Contact ;
        odrl:action acl:Read, acl:Write ;
        odrl:constraint [
            odrl:leftOperand dpv:Purpose ;
            odrl:operator odrl:isA ;
            odrl:rightOperand dpv:SellProductsToDataSubject
        ]
    ] . 